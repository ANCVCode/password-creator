import random
rastgele = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
 
kac_karakter = int(input("How many characters should the password consist of: "))
sifre = ""
for i in range(kac_karakter):
    sifre += random.choice(rastgele)
print("Your Password : ", sifre)
        
